;\r\n                this.istrue = true;\r\n            }\r\n            else{\r\n                let realc = {\r\n                    indexnum : mylength,\r\n                    class : 'incorrect',\r\n                }\r\n                this.myall.push(realc);\r\n                this.istrue = true;\r\n                this.istrue = false;\r\n            }\r\n            console.log(this.myall)\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n.correct{\r\n    color: blue;\r\n}\r\n.incorrect{\r\n    color: red;\r\n}\r\n.game{\r\n    width: 80%;\r\n    margin: auto;\r\n    background-color: gray;\r\n    position: relative;\r\n}\r\n.words{\r\n    filter: blur(4px);\r\n    color: rgb(95, 92, 92);\r\n    font-size: 20px;\r\n}\r\n.focused{\r\n    position: absolute;\r\n    inset: 0;\r\n    text-align: center;\r\n    \r\n}\r\n.game:focus .focused{\r\n    display: none;\r\n}\r\n.game:focus .words{\r\n    filter: blur(0px);\r\n}\r\n</style>"],"mappings":";AASA,eAAc;EACVA,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,UAAS,EAAI,EAAE;MACfC,MAAK,EAAI,qGAAqG;MAC9GC,MAAK,EAAI,EAAE;MACXC,OAAM,EAAI,IAAI;MACdC,MAAK,EAAI,IAAI;MACbC,KAAI,EAAI;IACZ;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAE;IACL,IAAI,CAACC,SAAS,CAAC;EACnB,CAAC;EACDC,OAAO,EAAC;IACJD,SAASA,CAAA,EAAE;MACP,IAAI,CAACP,UAAS,GAAI,IAAI,CAACC,MAAM,CAACQ,KAAK,CAAC,EAAE,CAAC;IAC3C,CAAC;IACDC,QAAQA,CAACC,KAAK,EAAC;MACX,IAAG,IAAI,CAACP,MAAM,EAAC;QACX,OAAO,SAAS;MACpB,OACI;QACA,OAAO,WAAW;MACtB;IAEJ,CAAC;IACDQ,IAAIA,CAACC,KAAK,EAAC;MACP,IAAIC,MAAK,GAAI,IAAI,CAACZ,MAAM;MACxB,IAAIa,QAAO,GAAI,IAAI,CAACb,MAAM,CAACc,MAAM;MACjC,IAAGH,KAAK,CAACI,GAAE,IAAK,WAAW,EAAC;QACxB,IAAIC,OAAM,GAAIJ,MAAM,CAACK,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;QACjC,IAAI,CAACjB,MAAK,GAAIgB,OAAM;MACxB,OACK,IAAGL,KAAK,CAACI,GAAE,IAAK,UAAS,IAAKJ,KAAK,CAACI,GAAE,IAAK,OAAM,IAAKJ,KAAK,CAACI,GAAE,IAAK,SAAQ,IAAKJ,KAAK,CAACI,GAAE,IAAK,UAAS,IAAKJ,KAAK,CAACI,GAAE,IAAK,KAAI,IAAKJ,KAAK,CAACI,GAAE,IAAK,SAAQ,EAAG;QAC1JG,OAAO,CAACC,GAAG,CAAC,0CAA0C;MAC1D,OACI;QACA,IAAI,CAACnB,MAAK,IAAIW,KAAK,CAACI